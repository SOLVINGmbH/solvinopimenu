{
  "$schema": "https://dev.office.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "solvin-opi-menu-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/solvinOpiMenu/SolvinOpiMenuApplicationCustomizer.js",
          "manifest": "./src/extensions/solvinOpiMenu/SolvinOpiMenuApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://code.jquery.com/jquery-3.3.0.min.js",
      "globalName": "jQuery"
    },
    "@microsoft/sp-module-loader": "node_modules/@microsoft/sp-module-loader/dist/sp-module-loader.js"
  },
  "localizedResources": {
    "SolvinOpiMenuApplicationCustomizerStrings": "lib/extensions/solvinOpiMenu/loc/{locale}.js"
  }
}
